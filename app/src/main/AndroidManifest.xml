<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="meliismyself.com.verticalseekbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VerticalSeekbar1" />
        <activity android:name=".VerticalSeekbar2" />
        <activity android:name=".VerticalSeekbar3" />
        <activity android:name=".VerticalSeekbar4"></activity>
    </application>

</manifest>
